{
  "name": "etrasamp3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:release": "yarn bundle && cd android && ./gradlew assembleRelease",
    "install:apk": "adb install android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "expo": "53.0.11",
    "expo-audio": "^0.4.6",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-background-fetch": "4.2.8",
    "react-native-background-geolocation": "4.18.7",
    "react-native-geolocation-service": "5.3.1",
    "react-native-permissions": "5.4.1",
    "react-native-reanimated": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.28.0",
    "jest": "^30.0.0",
    "patch-package": "^8.0.0",
    "prettier": "3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
